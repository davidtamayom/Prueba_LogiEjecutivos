<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logiejecutivos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LogiEjecutivos">
        <activity
            android:name="com.example.logiejecutivos.ui.filtro.FiltroActivity"
            android:exported="false" />
        <activity
            android:name="com.example.logiejecutivos.ui.citasdia.CitasDIaActivity"
            android:exported="false" />
        <activity
            android:name="com.example.logiejecutivos.ui.main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.logiejecutivos.ui.agendar.AgendarActivity"
            android:exported="true"></activity>
    </application>

</manifest>